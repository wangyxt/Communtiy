<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lixiangshequ.repository.UserMapper">
    <select id="findById" resultType="com.lixiangshequ.domain.User">
        select * from user where uid = $[id]
    </select>

    <select id="selectByTel" resultType="com.lixiangshequ.domain.User">
        select * from user where tel = #{tel}
    </select>
</mapper>